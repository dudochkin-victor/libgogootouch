        <actions>
            <sendkeysequence keysequence="Ctrl+D"> </sendkeysequence>
        </actions>
    </button>
    <button name="test2" group="test2" priority="0" showon="always" alignment="right" icon="" text=".COM" text_id="qtn_comm_test2">
        <actions>
            <sendstring string="www.nokia.com"> </sendstring>
            <showgroup group="test1"> </showgroup>
        </actions>
